{
  "name": "3mosqueteiros",
  "private": true,
  "scripts": {
    "watch_sass": "node-sass --watch styles/MyItCss/sass -o styles",
    "build_sass": "node-sass ./styles/MyItCss/sass/main.scss ./styles/main.css && npm run minify_css",
    "minify_css": "minify ./styles/main.css > ./styles/main.min.css",
    
    "dev": "next dev",
    "build": "npm run build_sass && next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "mongodb": "^4.1.3",
    "next": "12.0.9",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "eslint": "8.7.0",
    "eslint-config-next": "12.0.9",
    "minify": "^8.0.4",
    "node-sass": "^6.0.1",
    "sass": "^1.49.7",
    "sass-mq": "^5.0.1"
  }
}
